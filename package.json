{
  "name": "@powerfulyang/utils",
  "version": "0.9.1",
  "description": "common utils",
  "main": "dist/main/index.js",
  "module": "dist/module/index.js",
  "types": "dist/main/index.js",
  "author": {
    "name": "powerfulyang"
  },
  "repository": "https://github.com/powerfulyang/tools",
  "license": "MIT",
  "keywords": [
    "utils"
  ],
  "files": [
    "dist",
    "src"
  ],
  "browserslist": [
    "last 2 versions"
  ],
  "scripts": {
    "build": "rimraf dist && run-p build:*",
    "build:main": "tsc -p tsconfig.main.json",
    "build:module": "tsc -p tsconfig.module.json",
    "watch:build": "rimraf dist && tsc -p tsconfig.main.json --watch",
    "prettier": "prettier \"src/**/*.ts\" --write",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --format=pretty ./src --fix",
    "test": "jest"
  },
  "devDependencies": {
    "@powerfulyang/lint": "^0.4.0",
    "@types/ip": "^1.1.0",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "ip": "^1.1.5",
    "reflect-metadata": "^0.1.13"
  }
}
