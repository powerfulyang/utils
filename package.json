{
  "name": "@powerfulyang/utils",
  "version": "1.3.1",
  "description": "common utils",
  "main": "dist/main/index.js",
  "module": "dist/module/index.js",
  "author": {
    "name": "powerfulyang"
  },
  "repository": "https://github.com/powerfulyang/tools",
  "license": "MIT",
  "keywords": [
    "utils"
  ],
  "files": [
    "dist",
    "src"
  ],
  "browserslist": [
    "last 2 versions"
  ],
  "scripts": {
    "build": "rimraf dist && run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "watch:build": "rimraf dist && tsc -p tsconfig.json --watch",
    "prettier": "prettier \"src/**/*.ts\" --write",
    "lint:js": "eslint --ext .ts,.tsx  ./src --fix",
    "test": "jest",
    "tsc": "tsc --noEmit",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "precommit": "npm run test && npm run lint:js && npm run prettier && npm run tsc"
  },
  "devDependencies": {
    "@powerfulyang/lint": "^0.7.2",
    "@types/ip": "^1.1.0",
    "husky": "^7.0.4",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "ip": "^1.1.5",
    "reflect-metadata": "^0.1.13",
    "ts-toolbelt": "^9.6.0"
  }
}
