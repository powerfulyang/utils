{
  "name": "@powerfulyang/utils",
  "version": "1.4.0",
  "description": "common utils",
  "main": "dist/main/index.js",
  "module": "dist/module/index.js",
  "sideEffects": false,
  "author": {
    "name": "powerfulyang",
    "email": "i@powerfulyang.com",
    "url": "https://powerfulyang.com"
  },
  "homepage": "https://github.com/powerfulyang/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/powerfulyang/utils.git"
  },
  "bugs": {
    "url": "https://github.com/powerfulyang/utils/issues",
    "email": "i@powerfulyang.com"
  },
  "license": "MIT",
  "keywords": [
    "utils"
  ],
  "files": [
    "dist",
    "src"
  ],
  "browserslist": [
    "last 2 versions"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "run-p build:*",
    "build:source": "rollup -c",
    "build:types": "tsc -p tsconfig.types.json",
    "prettier": "prettier \"src/**/*.ts\" --write",
    "lint:js": "eslint --ext .ts ./src --fix",
    "test": "jest",
    "tsc-check": "tsc --noEmit",
    "postprepare": "husky install",
    "prepublishOnly": "npm run build",
    "precommit": "npm run test && npm run tsc-check && lint-staged"
  },
  "devDependencies": {
    "@powerfulyang/lint": "1.4.7",
    "@rollup/plugin-typescript": "8.3.2",
    "reflect-metadata": "0.1.13",
    "rollup": "2.70.2",
    "rxjs": "7.5.5",
    "tsconfig-paths-jest": "0.0.1"
  },
  "peerDependencies": {
    "reflect-metadata": ">=0.1.13",
    "rxjs": ">=7.5.5"
  },
  "peerDependenciesMeta": {
    "reflect-metadata": {
      "optional": true
    },
    "rxjs": {
      "optional": true
    }
  }
}
